{
  "name": "react-hotel",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "moment": "^2.29.4",
    "nodemon": "^2.0.20",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.4.1",
    "react-spinners": "^0.13.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm i && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "host": "node ./server/server.js",
    "dev": "nodemon ./server/server.js"
  },
  "proxy": "http://localhost:3001",
  "devDependencies": {
    "husky": "^2.3.0",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --verbose"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
